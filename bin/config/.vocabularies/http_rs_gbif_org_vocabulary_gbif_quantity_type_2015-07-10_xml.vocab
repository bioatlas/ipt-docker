  <concept dc:identifier="percentageOfSpecies"
           dc:URI="http://rs.gbif.org/vocabulary/gbif/quantityType/percentageOfSpecies"
           dc:relation=""
           dc:description="a measurement type where the quantity of a species in a sample is recorded as a percentage of the total individual count of all species">
    <preferred>
      <term dc:source="" dc:title="percentage of species" xml:lang="en"/>
      <term dc:source="" dc:title="Porcentaje de especies" xml:lang="es"/>
    </preferred>
    <alternative>
    </alternative>
  </concept>

